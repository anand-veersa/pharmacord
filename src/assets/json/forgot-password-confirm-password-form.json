        "pattern": "^(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.*[!@#$%^&*]+).{8,30}$"
      },
      "required": true,
      "options": []
    },
    {
      "name": "confirmNewPassword",
      "value": "",
      "display": true,
      "label": "Confirm New Password",
      "placeholder": "Enter your Confirm New Password",
      "type": "password",
      "reqErrorMsg": "Confirm New Password Required",
      "validityErrorMsg": "Invalid Confirm New Password",
      "validators": {
        "required": true,
        "max": 14,
        "min": 8,
        "pattern": "^(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.*[!@#$%^&*]+).{8,30}$"
      },
      "options": [],
